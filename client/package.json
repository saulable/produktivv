{
	"name": "meals",
	"version": "0.1.0",
	"private": true,
	"proxy": {
		"/api/*": {
			"target": "https://localhost:5000",
			"secure": false
		},
		"/auth/*": {
			"target": "https://localhost:5000",
			"secure": false
		}
	},
	"dependencies": {
		"animejs": "^2.2.0",
		"axios": "^0.18.0",
		"foundation-sites": "^6.5.0-rc.1",
		"jquery": "^3.3.1",
		"motion-ui": "^2.0.3",
		"node-sass-chokidar": "^1.3.0",
		"npm": "^6.1.0",
		"npm-run-all": "^4.1.3",
		"react": "^16.3.2",
		"react-autosuggest": "^9.3.4",
		"react-big-calendar": "^0.19.1",
		"react-day-picker": "^7.1.9",
		"react-dom": "^16.3.2",
		"react-redux": "^5.0.7",
		"react-router-dom": "^4.2.2",
		"react-scripts": "1.1.4",
		"react-stripe-checkout": "^2.6.3",
		"redux": "^3.7.2",
		"redux-form": "^7.3.0",
		"redux-thunk": "^2.2.0",
		"what-input": "^5.1.0"
	},
	"scripts": {
		"build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules/foundation-sites/scss --include-path ./node_modules/motion-ui src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules/foundation-sites/scss --include-path ./node_modules/motion-ui src/ -o src/ --watch --recursive",
		"start-js": "HTTPS=true react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build-js": "react-scripts build",
		"build": "npm-run-all build-css build-js",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"devDependencies": {
		"babel-plugin-transform-react-pug": "^6.0.0",
		"eslint": "^4.19.1",
		"eslint-plugin-react": "^7.7.0",
		"react-app-rewired": "^1.5.2"
	}
}
